  "scripts": {
    "ng": "ng",
    "start": "rimraf ./dist && concurrently \"nodemon ./src/ipc/main\" \"ng build --watch\"",
    "build:app": "ng build --prod --output-path ./build/dist",
    "build:delete": "rimraf ./build/dist/ipc",
    "build:bundle": "webpack",
    "build:ipc": "grunt task:buildIPC",
    "build:pack": "node ./scripts/archive.js",
    "build:clear": "rimraf ./build",
    "archive": "cross-env NODE_ENV=production npm-run-all build:*",
    "dev": "ng build --output-path ./build/dist&&grunt task:development&&ng serve --hmr -e=hmr",
    "update": "mocha --exit --no-deprecation&&grunt task:precommit&&grunt task:git",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
